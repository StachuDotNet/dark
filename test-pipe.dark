// Test pipe operator behavior
let testPipeWithAdd = 1L |> (+) 2L
// Expected with F# semantics: 2 + 1 = 3
// Old Darklang semantics: 1 + 2 = 3

let testPipeWithList =
  [ 1L; 2L; 3L ]
  |> Stdlib.List.map (fun x -> x + 1L)
// Expected: needs List.map to have fn as first param, list as second
// Current List.map signature: (list: List<'a>) (fn: 'a -> 'b)
// This will now fail because pipe sends: List.map((fun x -> x + 1), [1;2;3])
// But List.map expects: List.map([1;2;3], (fun x -> x + 1))

testPipeWithList
