module Darklang.Cli.Clear


let execute (state: AppState) (args: List<String>) : AppState =
  Builtin.stdoutClear ()
  { state with needsFullRedraw = true }


let complete (_state: AppState) (_args: List<String>) : List<String> =
  []


let help (state: AppState) : Unit =
  Stdlib.printLine "Usage: clear"
  Stdlib.printLine "Clear the terminal screen."
  Stdlib.printLine ""
  Stdlib.printLine "Aliases: cls"
