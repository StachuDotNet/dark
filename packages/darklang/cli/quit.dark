module Darklang.Cli.Quit


let execute (state: AppState) (_args: List<String>) : AppState =
  Stdlib.printLine "Goodbye!"
  { state with isExiting = true }


let complete (_state: AppState) (_args: List<String>) : List<String> =
  []


let help (state: AppState) : Unit =
  Stdlib.printLine "Usage: quit\nExit the CLI immediately.\nNo arguments required."

