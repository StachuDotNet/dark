module Darklang =
  module Cli =
    module Types =
      type Command = 
        | Help of List<String>
        | Quit
        | Unknown of String

      type Msg =
        | ProcessCommand of Command
        | Exit

      type AppState = 
        { isExiting: Bool
          lastCommand: String
          output: String }

      let initState () : AppState =
        AppState { isExiting = false; lastCommand = ""; output = "" }