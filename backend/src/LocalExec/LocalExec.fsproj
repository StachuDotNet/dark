<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <LangVersion>7.0</LangVersion>
    <OtherFlags>--warnaserror</OtherFlags>
    <!-- Publishing configuration -->
    <PublishReadyToRun>true</PublishReadyToRun>
    <SelfContained>false</SelfContained>
    <PublishSingleFile>false</PublishSingleFile>
    <PublishTrimmed>false</PublishTrimmed>
  </PropertyGroup>

  <!-- dependencies and code -->
  <Import Project="..\..\.paket\Paket.Restore.targets" />
  <ItemGroup><None Include="paket.references" /></ItemGroup>

  <ItemGroup>
    <ProjectReference Include="../LibExecution/LibExecution.fsproj" />
    <ProjectReference Include="../LibBackend/LibBackend.fsproj" />
    <ProjectReference Include="../Parser/Parser.fsproj" />
    <ProjectReference Include="../StdLibExecution/StdLibExecution.fsproj" />
    <ProjectReference Include="../StdLibCloudExecution/StdLibCloudExecution.fsproj" />
    <ProjectReference Include="../StdLibCli/StdLibCli.fsproj" />
    <ProjectReference Include="../StdLibDarkInternal/StdLibDarkInternal.fsproj" />

    <!-- only really needed for parsing -->
    <ProjectReference Include="../StdLibCloudExecution/StdLibCloudExecution.fsproj" />
    <ProjectReference Include="../../tests/TestUtils/TestUtils.fsproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="Libs/Packages.fs" />
    <Compile Include="Libs/Cli.fs" />
    <Compile Include="StdLib.fs" />
    <Compile Include="LocalExec.fs" />
  </ItemGroup>
</Project>
